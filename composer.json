{
    "name": "letoceiling-coder/media-manager",
    "description": "Независимый Vue компонент Media Manager для управления медиа файлами в Laravel приложениях",
    "keywords": [
        "laravel",
        "vue",
        "media",
        "manager",
        "files",
        "upload",
        "file-manager",
        "media-library"
    ],
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/letoceiling-coder/media-manager",
    "support": {
        "email": "dsc-23@yandex.ru",
        "issues": "https://github.com/letoceiling-coder/media-manager/issues",
        "source": "https://github.com/letoceiling-coder/media-manager"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/letoceiling-coder/media-manager.git"
        }
    ],
    "authors": [
        {
            "name": "letoceiling-coder",
            "email": "dsc-23@yandex.ru"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "illuminate/database": "^10.0|^11.0|^12.0",
        "illuminate/http": "^10.0|^11.0|^12.0",
        "illuminate/routing": "^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "LetoceilingCoder\\MediaManager\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LetoceilingCoder\\Media2\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LetoceilingCoder\\MediaManager\\MediaManagerServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}

